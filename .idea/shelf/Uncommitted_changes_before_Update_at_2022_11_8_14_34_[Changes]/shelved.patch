List from '../components/MedicineList.vue'\r\nimport MedicineDetail from '../components/MedicineDetail.vue'\r\nimport MyLocation from '../components/MyLocation.vue'\r\nimport AddMedicine from '@/components/AddMedicine.vue'\r\nimport router from '@/router'\r\n\r\nexport default {\r\n  name: 'DashboardView',\r\n  data() {\r\n    return {\r\n      displayFlag : false,\r\n      isPhone : true,\r\n      displayMode : null,\r\n      dataReady : false,\r\n      mediInfoId : null,\r\n      blurResult : null,\r\n      \r\n      \r\n      user : {\r\n        userName : this.$route.params.username,\r\n        nickName : '',\r\n        userEmail : '',\r\n        numMedicine : null,\r\n        locations : null,\r\n        userMatadata : null,\r\n      },\r\n\r\n      emptyDescription : \"No User Data\"\r\n    }\r\n  },\r\n  components: {\r\n    HeadBar,\r\n    SideBar,\r\n    SearchBar,\r\n    MedicineDetail,\r\n    MedicineList,\r\n    MyLocation,\r\n    MedicineDetail,\r\n    AddMedicine\r\n},\r\n  methods: {\r\n\r\n\r\n    openSideBar(){\r\n      this.displayFlag = true;\r\n    },\r\n\r\n\r\n    closeSideBar(){\r\n      this.displayFlag = false;\r\n    },\r\n    // handleRefresh(){\r\n    //   console.log(\"Refresh Component\")\r\n    //   this.displayMode = 'refresh'\r\n    // },\r\n\r\n    handleSwitch(mode){\r\n\r\n      console.log(\"switch to \" + mode.view)\r\n      if (mode.view == 'log_off'){\r\n        console.log(\"user log off\");\r\n        // ElMessage({\r\n        //               message: \"user log off\",\r\n        //               type: 'warning',\r\n        //           })\r\n        ElNotification.success({\r\n          title: 'Success',\r\n          message: 'User Log Off',\r\n          offset: 50,\r\n        })\r\n        this.$router.push(\"/\")\r\n        this.$store.commit(\"userLogOff\")\r\n      }\r\n      else {\r\n        if(mode.view == 'medi_info'){\r\n          console.log(\"Medicine info of : \" + this.mediInfoId)\r\n          this.mediInfoId 